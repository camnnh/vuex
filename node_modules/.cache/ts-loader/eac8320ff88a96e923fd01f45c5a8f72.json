;AADyC;AAAA,mBAGnC,KAAK,CAAC,IAAD,CAH8B;;AAAA;AAKzC,YAAA,MAAM,CAAC,YAAY,CAAC,UAAd,EAA0B,KAA1B,CAAN;AACA,YAAA,MAAM,CAAC,YAAY,CAAC,QAAd,EAAwB,CAC5B;AACE,cAAA,EAAE,EAAE,CADN;AAEE,cAAA,IAAI,EAAE,yCAFR;AAGE,cAAA,SAAS,EAAE;AAHb,aAD4B,EAM5B;AACE,cAAA,EAAE,EAAE,CADN;AAEE,cAAA,IAAI,EAAE,eAFR;AAGE,cAAA,SAAS,EAAE;AAHb,aAN4B,EAW5B;AACE,cAAA,EAAE,EAAE,CADN;AAEE,cAAA,IAAI,EAAE,eAFR;AAGE,cAAA,SAAS,EAAE;AAHb,aAX4B,CAAxB,CAAN;;AANyC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAuB1C,CAxBiB,CAAb","sourcesContent":["import { ActionContext, ActionTree } from 'vuex'\nimport { Mutations, MutationType } from './mutations'\nimport { State } from './state'\n\nexport enum ActionTypes {\n  GetTodoItems = 'GET_ITEMS'\n}\n\ntype ActionAugments = Omit<ActionContext<State, State>, 'commit'> & {\n  commit<K extends keyof Mutations>(\n    key: K,\n    payload: Parameters<Mutations[K]>[1]\n  ): ReturnType<Mutations[K]>\n}\n\nexport type Actions = {\n  [ActionTypes.GetTodoItems](context: ActionAugments): void\n}\n\nconst sleep = (ms: number) => new Promise(resolve => setTimeout(resolve, ms))\n\nexport const actions: ActionTree<State, State> & Actions = {\n  async [ActionTypes.GetTodoItems]({ commit }) {\n    commit(MutationType.SetLoading, true)\n\n    await sleep(1000)\n\n    commit(MutationType.SetLoading, false)\n    commit(MutationType.SetItems, [\n      {\n        id: 1,\n        text: 'Create awesome Vue 3 with Vuex 4 video!',\n        completed: true\n      },\n      {\n        id: 2,\n        text: 'Vuex 5 video!',\n        completed: false\n      },\n      {\n        id: 3,\n        text: 'Vuex 6 video!',\n        completed: false\n      }\n    ])\n  }\n}\n"],"sourceRoot":""}]}