 true },\n    completed: { type: Boolean, required: true }\n  },\n  setup(props) {\n    const store = useStore()\n\n    const toggleCompletion = () => {\n      store.commit(MutationType.CompleteItem, {\n        id: props.id,\n        completed: !props.completed\n      })\n    }\n\n    return { toggleCompletion }\n  }\n})\n</script>\n"]}]}